'.source.pwn, .source.inc':
#common
  '#include <>':
    'prefix': '#inc'
    'body': '#include <${1:}>'
  '#include ""':
    'prefix': '#inc'
    'body': '#include "${1:.inc}"'
  'main()':
    'prefix': 'main'
    'body': 'main() {\n\t${1:/* code */}\n\treturn 1;\n}'
  'for loop':
    'prefix': 'for'
    'body': 'for (new ${1:i} = 0; ${1:i} < ${2:count}; ${1:i}${3:++}) {\n\t${4:/* code */}\n}'
  'include guard':
    'prefix': 'incgrd'
    'body': '#if !defined _INC_${1:SYMBOL}\n\t#endinput\n#endif\n#define _INC_$1'
  'do while loop':
    'prefix': 'do'
    'body': 'do {\n\t${0:/* code */}\n} while(${1:/* condition */});'
  'while loop':
    'prefix': 'while'
    'body': 'while (${1:/* condition */}) {\n\t${2:/* code */}\n}'
  'if condition':
    'prefix': 'if'
    'body': 'if (${1:/* condition */}) {\n\t${2:/* code */}\n}'
  'if else':
    'prefix': 'ife'
    'body': 'if (${1:/* condition */}) {\n\t${2:/* code */}\n}\nelse {\n\t${3:/* code */}\n}'
  'if elseif':
    'prefix': 'iff'
    'body': 'if (${1:/* condition */}) {\n\t${2:/* code */}\n}\nelse if (${3:/* condition */}) {\n\t${4:/* code */}\n}'
  'if elseif else':
    'prefix': 'iffe'
    'body': 'if (${1:/* condition */}) {\n\t${2:/* code */}\n}\nelse if (${3:/* condition */}) {\n\t${4:/* code */}\n}\nelse {\n\t${5:/* code */}\n}'
  'switch statement':
    'prefix': 'switch'
    'body': 'switch (${1:/* expression */}) {\n\tcase ${2:/* value */}:\n}'
  'case':
    'prefix': 'case'
    'body': 'case ${1:/* value */}:$0'
  'printf':
    'prefix': 'printf'
    'body': 'printf("${1:%s}\\\\n", ${2:output});$3'
  'print':
    'prefix': 'print'
    'body': 'print("${1:output}\\\\n");$2'
  'random':
    'prefix': 'random'
    'body': 'random(${1:max})$2'
  'minrandom':
    'prefix': 'minrandom'
    'body': '(random(${1:min} - ${2:max}) + $2)'
  'getdate':
    'prefix': 'getdate'
    'body': 'getdate(${1:year}, ${2:month}, ${3:day})$4'
  'gettime':
    'prefix': 'gettime'
    'body': 'gettime(${1:hour}, ${2:minute}, ${3:second})$4'
  'tolower':
    'prefix': 'tolower'
    'body': 'tolower(${1:c});$2'
  'toupper':
    'prefix': 'toupper'
    'body': 'toupper(${1:c});$2'
  'valstr':
    'prefix': 'valstr'
    'body': 'valstr(${1:dest[]}, ${2:value});$3'
  'strcat':
    'prefix': 'strcat'
    'body': 'strcat(${1:dest[]}, ${2:const source[]});$3'
  'ispacked':
    'prefix': 'ispacked'
    'body': 'ispacked(${1:const string[]});$2'
  'strpack':
    'prefix': 'strpack'
    'body': 'strpack(${1:dest[]}, ${2:const source[]});$3'
  'strunpack':
    'prefix': 'strunpack'
    'body': 'strunpack(${1:dest[]}, ${2:const source[]});$3'
  'strcmp':
    'prefix': 'strcmp'
    'body': 'strcmp(${1:const string1[]}, ${2:const string2[]})$3'
  'strdel':
    'prefix': 'strdel'
    'body': 'strdel(${1:string[]}, ${2:start}, ${3:end});$4'
  'strfind':
    'prefix': 'strfind'
    'body': 'strfind(${1:const string[]}, ${2:const sub[]})$3'
  'strins':
    'prefix': 'strins'
    'body': 'strins(${1:string[]}, ${2:const sub[]}, ${3:pos});$4'
  'strlen':
    'prefix': 'strlen'
    'body': 'strlen(${1:const string[]})$2'
  'strval':
    'prefix': 'strval'
    'body': 'strval(${1:const string[]})$2'
  'strmid':
    'prefix': 'strmid'
    'body': 'strmid(${1:dest[]}, ${2:const source[]}, ${3:start}, ${4:end});$5'
  'stock':
    'prefix': 'stock'
    'body': 'stock ${1:function}(${2:/* arguments */}) {\n\t${4:/* code */}\n\treturn ${3:1};\n}'
  'public':
    'prefix': 'public'
    'body': 'public ${1:function}(${2:/* arguments */}) {\n\t${4:/* code */}\n\treturn ${3:1};\n}'
  'forward':
     'prefix': 'forward'
     'body': 'forward ${1:function}(${2:/* arguments */});'
  'atan':
     'prefix': 'atan'
     'body': 'atan(${1:Float:value})'
  'floatsin':
     'prefix': 'floatsin'
     'body': 'floatsin(${1:Float:value}, ${2:anglemode:mode=radian})'
  'floattan':
     'prefix': 'floattan'
     'body': 'floattan(${1:Float:value}, ${2:anglemode:mode=radian})'
  'floatcos':
     'prefix': 'floatcos'
     'body': 'floatcos(${1:Float:value}, ${2:anglemode:mode=radian})'
  'format':
     'prefix': 'format'
     'body': 'format(${1:output[]}, sizeof(${1:}), ${2:const format[]}, ${3:{Float,_}:...});${4:}'
  'write file':
     'prefix': 'wfile'
     'body': 'new File:${1:file} = fopen(${2:filename.extension}, io_write);\nif(${1:file}) {\n\tfwrite(${1:file}, ${3:text_to_write});\n\tfclose(${1:file});\n}${4:}'
